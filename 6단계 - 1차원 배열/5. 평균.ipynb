    "n = int(input())\n",
    "score = list(map(int,input().split()))\n",
    "m = max(score)\n",
    "\n",
    "for i in range(n):\n",
    "  score[i]= score[i]/m*100\n",
    "  avg = sum(score)/n\n",
    "print(round(avg,2))\n",
    "\n"
